{
    "manifest_version": 3,
    "name": "Custom CSS Add-On",
    "version": "1.0",
    "description": "Change the CSS of a specific webpage.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["https://chatgpt.com/*"], 
        "css": ["style.css"]
      }
    ]
  }
  